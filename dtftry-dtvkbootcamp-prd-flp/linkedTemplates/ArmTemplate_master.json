{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dtftry-dtvkbootcamp-prd-flp"
		},
		"ls_datalk_strg_bootcamp_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalk_strg_bootcamp'"
		},
		"lnkdsvs_kv_bootcamp_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-dtvk-botcamp-prd-flp.vault.azure.net/"
		},
		"ls_az_dtbcks_bootcamp_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0507-233833-1cwhof13"
		},
		"ls_datalk_strg_bootcamp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsdtvkbootcampprdflp.dfs.core.windows.net/"
		},
		"ls_http_api_bootcamp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummyjson.com/users"
		},
		"ls_webtable_bootcamp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.dieese.org.br/analisecestabasica/salarioMinimo.html"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dtftry-dtvkbootcamp-prd-flp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_datalk_strg_bootcamp_accountKey": {
						"value": "[parameters('ls_datalk_strg_bootcamp_accountKey')]"
					},
					"lnkdsvs_kv_bootcamp_properties_typeProperties_baseUrl": {
						"value": "[parameters('lnkdsvs_kv_bootcamp_properties_typeProperties_baseUrl')]"
					},
					"ls_datalk_strg_bootcamp_properties_typeProperties_url": {
						"value": "[parameters('ls_datalk_strg_bootcamp_properties_typeProperties_url')]"
					},
					"ls_http_api_bootcamp_properties_typeProperties_url": {
						"value": "[parameters('ls_http_api_bootcamp_properties_typeProperties_url')]"
					},
					"ls_az_dtbcks_bootcamp_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_az_dtbcks_bootcamp_properties_typeProperties_existingClusterId')]"
					},
					"ls_webtable_bootcamp_properties_typeProperties_url": {
						"value": "[parameters('ls_webtable_bootcamp_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}